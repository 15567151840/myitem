<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.eduuser.mapper.PersonsMapper">
  <select id="getWantInfo"  resultType="java.lang.String">
      SELECT p.LastName  from persons p left join user u  on p.id = u.id
      where p.id=#{id}
  </select>
</mapper>
